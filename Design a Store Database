SELECT * FROM Furniture ORDER BY price;
SELECT AVG(price) FROM Furniture;
SELECT name, MAX(price) FROM Furniture;
SELECT name FROM Furniture WHERE quantity > 5;
SELECT name FROM Furniture WHERE color = "brown";
